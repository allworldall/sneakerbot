<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.cameronb.bot.Controller">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <SplitPane dividerPositions="0.3" prefHeight="343.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
        <items>
            <VBox prefHeight="309.0" prefWidth="188.0">
               <children>
                  <HBox prefHeight="48.0" prefWidth="188.0">
                     <children>
                        <Label prefHeight="33.0" prefWidth="75.0" text="Tasks">
                           <padding>
                              <Insets left="5.0" />
                           </padding>
                        </Label>
                        <Region prefHeight="33.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="addTaskButton" mnemonicParsing="false" onAction="#openTaskCreation" text="Add Task">
                           <HBox.margin>
                              <Insets bottom="5.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <ListView fx:id="taskListView" prefHeight="309.0" prefWidth="190.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <SplitPane dividerPositions="0.5, 0.77" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
              <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" />
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="205.0" prefWidth="397.0">
                     <children>
                        <HBox layoutX="2.0" prefHeight="43.0" prefWidth="448.0" spacing="45.0">
                           <children>
                              <Button fx:id="startTasksButton" mnemonicParsing="false" onAction="#startTasks" prefHeight="25.0" prefWidth="116.0" text="Start Tasks">
                                 <HBox.margin>
                                    <Insets left="5.0" top="9.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="stopTasksButton" disable="true" mnemonicParsing="false" onAction="#stopTasks" prefHeight="25.0" prefWidth="116.0" text="Stop">
                                 <HBox.margin>
                                    <Insets top="9.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </AnchorPane>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="consoleOutput" editable="false" prefHeight="156.0" prefWidth="448.0" />
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
        </items>
      </SplitPane>
      <HBox prefHeight="44.0" prefWidth="640.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="leftStatus" prefHeight="41.0" prefWidth="191.0" text="some status" HBox.hgrow="NEVER">
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Label fx:id="rightStatus" alignment="CENTER_RIGHT" prefHeight="41.0" prefWidth="191.0" text="some status" HBox.hgrow="NEVER">
               <padding>
                  <Insets right="10.0" />
               </padding>
            </Label>
         </children>
      </HBox>
  </children>
</VBox>
